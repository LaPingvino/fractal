<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="ContentMessageSenderName" parent="AdwBin">
    <property name="focusable">True</property>
    <child>
      <object class="GtkLabel" id="label">
        <property name="ellipsize">end</property>
        <binding name="label">
          <lookup name="disambiguated-name">
            <lookup name="sender">ContentMessageSenderName</lookup>
          </lookup>
        </binding>
        <style>
          <class name="heading"/>
          <class name="accent"/>
        </style>
      </object>
    </child>
  </template>
</interface>
