<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="AccountSettings" parent="AdwPreferencesDialog">
    <property name="title" translatable="yes">Account Settings</property>
    <property name="content-height">780</property>
    <style>
      <class name="account-settings"/>
    </style>
    <child>
      <object class="AccountSettingsGeneralPage">
        <property name="session" bind-source="AccountSettings" bind-property="session" bind-flags="sync-create"/>
        <property name="account-settings">AccountSettings</property>
      </object>
    </child>
    <child>
      <object class="NotificationsPage">
        <binding name="notifications-settings">
          <lookup name="settings">
            <lookup name="notifications">
              <lookup name="session">AccountSettings</lookup>
            </lookup>
          </lookup>
        </binding>
      </object>
    </child>
    <child>
      <object class="SafetyPage">
        <property name="session" bind-source="AccountSettings" bind-property="session" bind-flags="sync-create"/>
      </object>
    </child>
    <child>
      <object class="EncryptionPage">
        <property name="session" bind-source="AccountSettings" bind-property="session" bind-flags="sync-create"/>
      </object>
    </child>
  </template>
</interface>
